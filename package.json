{
  "name": "rank-factory",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun run dev:server & bun run dev:client",
    "dev:server": "bun run server/index.ts",
    "dev:client": "vite --config client/vite.config.ts",
    "build": "bun run build:client",
    "build:client": "vite build --config client/vite.config.ts",
    "preview": "wrangler pages dev dist/client",
    "deploy": "export $(cat .env | grep -E '^CLOUDFLARE_' | xargs) && bun run build:client && bunx wrangler pages deploy dist/client",
    "start": "bun run server/index.ts",
    "start:prod": "bun run build && bun run start",
    "seed": "bun run scripts/seed/seed.ts",
    "lint": "eslint client/src server"
  },
